<?xml version="1.0" encoding="UTF-8"?>
	<!-- 第二个切换卡 -->

<ScrollView android:id="@+id/userface02_ScrollView"
	android:layout_width="fill_parent" 
	android:layout_height="fill_parent"
	xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:paddingLeft="8dp"
    android:paddingRight="8dp"
	>

	<RelativeLayout
		android:id="@+id/info_linearLayout02"
		android:layout_width="fill_parent" 
		android:layout_height="fill_parent"
		>
	
	
	<RelativeLayout 
	    android:id="@+id/RelativeLayout1"
	    android:layout_width="180dp" 
	    android:layout_height="200dp"
	    android:background="#FFFFFF"
	    >

     <TextView 
        android:id="@+id/textView_motor1_setforward"
        android:layout_marginTop="35dp"
        android:paddingLeft="35dp"
        android:text="向前速度："
        android:textSize="20dp"
        android:textColor="#1C1C1C"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        />
    
    <EditText 
        android:id="@+id/editText_motor1_setforward"
        android:layout_toRightOf="@id/textView_motor1_setforward"
        android:layout_alignBaseline="@id/textView_motor1_setforward"
        android:textSize="20dp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:inputType="number"
        android:text="1"
        />
    
    <TextView 
        android:id="@+id/textView_motor1_setbackward"
        android:layout_marginTop="75dp"
        android:paddingLeft="35dp"
        android:layout_below="@id/textView_motor1_setforward"
        android:text="向后速度："
        android:textSize="20dp"
        android:textColor="#1C1C1C"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        />
    
    <EditText 
        android:id="@+id/editText_motor1_setbackward"
        android:layout_toRightOf="@id/textView_motor1_setbackward"
        android:layout_alignBaseline="@id/textView_motor1_setbackward"
        android:textSize="20dp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="-1"
        />

</RelativeLayout>
 
//页面第二个部分
	<RelativeLayout 
	    android:id="@+id/RelativeLayout2"
	    android:layout_alignParentTop="true"
	    android:layout_toRightOf="@id/RelativeLayout1"
	    android:layout_width="fill_parent" 
	    android:layout_height="200dp"
	    android:background="#FFFFFF"
	    >
    
	    <Button 
        android:id="@+id/button_motor1_forward"
        android:layout_marginTop="30dp"
        android:layout_centerHorizontal="true"
        android:layout_width="40dp"
        android:layout_height="40dp"
        android:background="@drawable/forward"
        />
    
    
    <Button 
        android:layout_below="@id/button_motor1_forward"
        android:layout_marginTop="10dp"
        android:layout_alignLeft="@id/button_motor1_forward"
        android:id="@+id/button_motor1_stop"
        android:layout_width="40dp"
        android:layout_height="40dp"
        android:background="@drawable/stop"
        />

    
    <Button 
        android:layout_below="@id/button_motor1_stop"
        android:layout_alignLeft="@id/button_motor1_stop"
        android:layout_marginTop="10dp"
        android:id="@+id/button_motor1_backward"
        android:layout_width="40dp"
        android:layout_height="40dp"
        android:background="@drawable/backward"
        />
	    
	    </RelativeLayout>
	    
//页面第三个部分	
 <RelativeLayout 
        android:id="@+id/RelativeLayout3"
        android:layout_below="@id/RelativeLayout1"
        android:layout_marginTop="30dp"
