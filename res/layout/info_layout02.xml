    android:textSize="14dp"
    />
    
<EditText 
    android:id="@+id/editText_motor1_PMW_min"
    android:layout_alignBaseline="@id/textView_motor1_PMW"   
    android:layout_toRightOf="@id/textView_motor1_PMW"
